<?xml version="1.0" encoding="UTF-8"?>
<assembly>
    <id>assembly</id>
    <baseDirectory>socks5-netty</baseDirectory>
    <formats>
        <format>dir</format>
        <!--<format>tar.gz</format>-->
    </formats>
    <dependencySets>
        <dependencySet>
            <useProjectArtifact>true</useProjectArtifact>
            <outputDirectory>lib</outputDirectory>
        </dependencySet>
    </dependencySets>
    <fileSets>
        <fileSet>
            <includes>
                <include>bin/*</include>
                <include>lib/*</include>
            </includes>
        </fileSet>
        <fileSet>
            <directory>src/main/resources</directory>
            <outputDirectory>/config</outputDirectory>
        </fileSet>
    </fileSets>
</assembly>
